{
  "name": "task-tracker-cli",
  "version": "1.0.0",
  "description": "Utility for creating and managing tasks from the cli",
  "main": "start.ts",
  "type": "module",
  "bin": {
    "task-cli": "/index.js"
  },
  "scripts": {
    "compile:watch": "npx tsc -w",
    "compile:rebuild": "tsc --build --clean && tsc",
    "task-cli": "node ./dist/src/index.js",
    "test": "npm run compile:rebuild && node ./dist/test/index.js"
  },
  "keywords": [
    "task-cli",
    "task",
    "cli",
    "tasks"
  ],
  "author": "fferdico",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.7.0",
    "fs-extra": "^11.3.2",
    "typescript": "^5.9.3"
  }
}
